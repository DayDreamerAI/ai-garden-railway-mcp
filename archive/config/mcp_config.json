{
  "mcpServers": {
    "ai-garden": {
      "command": "python",
      "args": ["-u", "mcp_server.py"],
      "env": {
        "NEO4J_URI": "${NEO4J_URI}",
        "NEO4J_USERNAME": "${NEO4J_USERNAME}",
        "NEO4J_PASSWORD": "${NEO4J_PASSWORD}",
        "RAILWAY_BEARER_TOKEN": "${RAILWAY_BEARER_TOKEN}",
        "PORT": "8081"
      },
      "transport": "sse"
    }
  }
}